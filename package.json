{
  "name": "real-estate-management",
  "version": "1.0.0",
  "description": "Complete Real Estate Management System for plot bookings, payments, and broker management",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "pm2-runtime start index.js --name dsinfra",
    "dev": "nodemon -r dotenv/config index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "migrate:run": "node -e \"require('./utils/migrate').runMigrations().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "migrate:rollback": "node -e \"require('./utils/migrate').rollbackMigration().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\"",
    "migrate:check": "node -e \"require('./utils/migrate').checkMigrationStatus().then(() => process.exit(0)).catch(err => { console.error(err); process.exit(1); })\""
  },
  "keywords": [
    "real-estate",
    "booking",
    "payment",
    "broker",
    "management"
  ],
  "author": "sumit.yadav",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.15.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "razorpay": "^2.9.2",
    "sequelize": "^6.37.7",
    "umzug": "^3.8.2"
  },
  "optionalDependencies": {
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.3"
  }
}
